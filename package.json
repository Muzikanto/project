{
  "name": "muzikanto",
  "version": "0.1.0",
  "private": true,
  "description": "ssr react app",
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "start": "./node_modules/.bin/cross-env NODE_ENV=development webpack --config .config/webpack.server.js --watch",
    "start:production": "./node_modules/.bin/cross-env NODE_ENV=production node --optimize_for_size --max_old_space_size=460 index.js",
    "start:docker": "docker run -d --rm --name my-running-app muzikanto-app",
    "build": "npm run build:clean && npm run build:server && npm run build:client",
    "build:clean": "rm -r --force dist",
    "build:server": "./node_modules/.bin/cross-env NODE_ENV=production webpack --config .config/webpack.server.js",
    "build:client": "react-app-rewired build --config-overrides ./.config/webpack.client.js",
    "build:docker": "rm -r doc && docker build -t muzikanto-app",
    "test": "jest",
    "load-films": "hermione ./hermione/hack.hermione.js --wait-timeout=100000 --config=./hermione/.hermione.conf.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test",
      "...": "..."
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Muzikanto/project.git"
  },
  "contributors": [
    "Muzikanto"
  ],
