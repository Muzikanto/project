  "private": true,
  "description": "ssr react app",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "./node_modules/.bin/cross-env NODE_ENV=development webpack --config .config/webpack.server.js --watch",
    "start:production": "./node_modules/.bin/cross-env NODE_ENV=production node dist/server",
    "start:docker": "docker run -d --rm --name my-running-app muzikanto-app",
    "build": "npm run build:clean && npm run build:server && npm run build:client && rm -r ./dist/Components",
    "build:clean": "rm -r --force dist",
    "build:server": "./node_modules/.bin/cross-env NODE_ENV=production webpack --config .config/webpack.server.js",
    "build:client": "react-app-rewired build --config-overrides ./.config/webpack.client.js",
    "build:docker": "rm -r doc && docker build -t muzikanto-app",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test",
      "...": "..."
    }
  },
